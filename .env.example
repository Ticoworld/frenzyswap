# FrenzySwap Environment Variables Template
# Copy this to .env.local and fill in your values

# Supabase Configuration (Required for Analytics)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Wallet Access Control
NEXT_PUBLIC_ALLOWED_WALLETS=wallet1,wallet2,wallet3
NEXT_PUBLIC_ADMIN_WALLETS=founder_wallet_address,dev_wallet_address

# Analytics Access Control (Optional - falls back to ADMIN_WALLETS)
NEXT_PUBLIC_ANALYTICS_AUTHORIZED_WALLETS=founder_wallet_address,analytics_team_wallet

# RPC Configuration
NEXT_PUBLIC_RPC_URL=https://api.mainnet-beta.solana.com
NEXT_PUBLIC_REFERRAL_ACCOUNT=your_referral_account_address

# Example filled values:
# NEXT_PUBLIC_SUPABASE_URL=https://abcdefgh.supabase.co
# NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
# NEXT_PUBLIC_ALLOWED_WALLETS=EPjFWdd5AufqSSqeM2qN1xzybapC8G4wEGGkZwyTDt1v,9WzDXwBbmkg8ZTbNMqUxvQRAyrZzDsGYdLVL9zYtAWWM
# NEXT_PUBLIC_ADMIN_WALLETS=EPjFWdd5AufqSSqeM2qN1xzybapC8G4wEGGkZwyTDt1v
# NEXT_PUBLIC_RPC_URL=https://api.mainnet-beta.solana.com
